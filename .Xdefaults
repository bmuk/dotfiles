URxvt*saveLines: 512

URxvt.letterSpace: -1

URxvt*tintColor: Blue

URxvt*depth: 32
URxvt*background: [90]#000000
URxvt*foreground: White

! black
URxvt*color0  : #2e3436
URxvt*color8  : #555753
! red
URxvt*color1  : #cc0000
URxvt*color9  : #ef2929
! green
URxvt*color2  : #4e9a06
URxvt*color10 : #8ae234
! yellow
URxvt*color3  : #c4a000
URxvt*color11 : #fce94f
! blue
URxvt*color4  : #3465a4
URxvt*color12 : #729fcf
! magenta
URxvt*color5  : #75507b
URxvt*color13 : #ad7fa8
! cyan
URxvt*color6  : #06989a
URxvt*color14 : #34e2e2
! white
URxvt*color7  : #d9d7cf
URxvt*color15 : #f0eeec

URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: rxvt

URxvt*font: xft:Liberation Mono for Powerline:pixelsize=15:antialias=true:hinting=true
URxvt*boldFont: xft:Liberation Mono for Powerline:pixelsize=15:antialias=true:bold:hinting=true
URxvt*italicFont: xft:Liberation Mono for Powerline:pixelsize=15:antialias=true:italic:autohint=true:hinting=true
URxvt*bolditalicFont: xft:Liberation Mono for Powerline:pixelsize=15:antialias=true:bold:italic:autohint=true:hinting=true

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

! black
*xterm*color0 : #2e3436
*xterm*color8 : #555753
! red
*xterm*color1 : #cc0000
*xterm*color9 : #ef2929
! green
*xterm*color2 : #4e9a06
*xterm*color10 : #8ae234
! yellow
*xterm*color3 : #c4a000
*xterm*color11 : #fce94f
! blue
*xterm*color4 : #3465a4
*xterm*color12 : #729fcf
! magenta
*xterm*color5 : #75507b
*xterm*color13 : #ad7fa8
! cyan
*xterm*color6 : #06989a
*xterm*color14 : #34e2e2
! white
*xterm*scrollBar: false
*xterm*scrollstyle: rxvt
*xterm*faceName: Liberation Mono for Powerline
*xterm*faceSize: 13
*xterm*cursorColor: white
*xterm*utf8: 2
*xterm*termName: xterm-256color
!*xterm.VT100.geometry: 89x29
*xterm*metaSendsEscape: true
*xterm*highlightSelection: true
*xterm*trimSelection: true
*xterm*cutNewline: false
*xterm*charClass: 3:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48,58:48,63:48,61:48,44:48,38:48,59:48
*xterm*saveLines: 512
*xterm.letterSpace: -1
*xterm*tintColor: Blue
!*xterm*shading: 40
*xterm*background: #222222
*xterm*foreground: white
*xterm*locale: true

XTerm*VT100.translations: #override \
  Shift <Key>Up: scroll-back(1) \n\
  Shift <Key>Down: scroll-forw(1) \n\
  Shift Ctrl <Key>V: insert-selection(CLIPBOARD) \n
